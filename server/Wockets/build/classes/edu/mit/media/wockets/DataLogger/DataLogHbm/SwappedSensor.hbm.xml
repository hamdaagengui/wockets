<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping default-lazy="false">
	<class name="edu.mit.media.wockets.DataLogger.DataLoggerBean.SwappedSensor" table="Swapped_Sensor">
		<composite-id>
			<key-property name="swappingId" type="int" column="Swapping_Id"/>
			<key-property name="macId" type="string" column="Mac_Id"/>
		</composite-id>
		
		<property name="sensorPlacement" type="string" column="Sensor_Placement"/>		

	</class>
	
</hibernate-mapping>