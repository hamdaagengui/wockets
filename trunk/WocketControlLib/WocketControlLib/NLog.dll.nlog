<nlog xmlns="http://www.nlog-project.org/schemas/NLog.xsd" 
      xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"> 
      <targets>
		<target name="f1" xsi:type="File" fileName="${basedir}\WocketLog.txt"/>
      </targets>
      
      <rules>
		<logger name="WocketControlLib.*" minlevel = "Debug" maxlevel = "Fatal" writeTo = "f1" />
      </rules>
</nlog> 
