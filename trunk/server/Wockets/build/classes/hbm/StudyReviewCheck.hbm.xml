<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping default-lazy="false">
	<class name="edu.mit.media.wockets.Beans.StudyReviewCheck" table="STUDY_REVIEW_CHECKLIST">
		<composite-id>
			<key-property name="studyId" type="string" column="Study_Id"/>
			<key-property name="checkId" type="int" column="Check_Id"/>
		</composite-id>
	</class>
</hibernate-mapping>