<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping default-lazy="false">
	<class name="edu.mit.media.wockets.Beans.Sim" table="SIMCARDS">

		 
		<id name="phoneNbr" type="string" column="Phone_Number"/>
  		<property name="carrier" type="string" column="Carrier"/>		
		<property name="contractExpDate" type="string" column="Contract_Exp"/>
		<property name="isAssigned" type="char" column="IsAssigned"/>
		<property name="notes" type="string" column="Notes"/>

	</class>
	
</hibernate-mapping>