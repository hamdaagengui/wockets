<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping default-lazy="false">
	<class name="edu.mit.media.wockets.Beans.Phone" table="PHONE">

		 
		<id name="IMEI" type="string" column="IMEI"/>
  		<property name="phoneModel" type="string" column="Phone_Model"/>		
		<property name="platform" type="string" column="Platform"/>
		<property name="OSVersion" type="string" column="OS_Version"/>
		<property name="appVersion" type="string" column="App_Version"/>
		<property name="isAssigned" type="char" column="IsAssigned"/>

	</class>
	
</hibernate-mapping>